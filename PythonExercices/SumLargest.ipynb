   "source": [
    "import numpy as np\n",
    "def sumLargestNumbers(list):\n",
    "   a = np.array(list)\n",
    "   maxPos = np.argmax(a)\n",
    "   max1 = a.max()\n",
    "   a[maxPos] = 0\n",
    "   max2 = a.max()\n",
    "   return max1 + max2"
   ]
  },
